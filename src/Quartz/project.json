{
  "version": "3.0.0-alpha1-*",
  "description": "Quartz.NET",
  "authors": [
    "Marko Lahma"
  ],
  "packOptions": {
    "tags": [
      "scheduling", "tasks", "jobs", "triggers", "scheduler", "threading"
    ],
    "projectUrl": "http://www.quartz-scheduler.net/",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
    "copyright": "Copyright Marko Lahma",
    "owners": [ "Marko Lahma" ],
    "iconUrl": "",
    "requireLicenseAcceptance": false,
    "summary": "Quartz.NET Scheduling Framework for .NET Platform"
  },
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "buildOptions": {
    "compilationOptions": { 
      "keyFile": "..\\quartz.net.snk"
    },
    "warningsAsErrors": true,
    "define": [
      "LIBLOG_PUBLIC",
      "LIBLOG_PORTABLE"
    ],
    "sources": {
      "compile": "../AssemblyInfo.cs"
    },
    "embed": {
      "includeFiles": [
        "quartz.config",
        "Impl/AdoJobStore/Common/dbproviders.properties",
        "Impl/AdoJobStore/Common/dbproviders.netstandard15.properties"
      ]
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [
          "APPDOMAINS",
          "BINARY_SERIALIZATION",
          "CONFIGURATION",
          "HTTPCONTEXT",
          "MAIL",
          "REMOTING",
          "THREAD_APARTMENTSTATE",
          "THREAD_INTERRUPTION",
          "THREAD_PRIORITY",
          "WINDOWS_PROCESS",
          "WINDOWS_THREADPOOL",
          "XML_SCHEMA"
        ]
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Net.Http": "4.0.0.0",
        "System.Runtime.Remoting": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Web": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      }
    },
    "netstandard1.6": {
      "imports": [
        "dotnet55",
        "portable-net45+win8"
      ],
      "buildOptions": {
        "debugType": "portable",
        "define": [
          "APPCONTEXT",
          "BUG_7552",
          "NETSTANDARD15_DBPROVIDERS"
        ]
      },
      "dependencies": {
        "System.AppContext": "4.1.0",
        "System.Collections.Concurrent": "4.0.12",
        "System.Collections.NonGeneric": "4.0.1",
        "System.Collections.Specialized": "4.0.1",
        "System.ComponentModel.TypeConverter": "4.1.0",
        "System.Data.SqlClient": "4.1.0",
        "System.Diagnostics.Process": "4.1.0",
        "System.Diagnostics.StackTrace": "4.0.1",
        "System.Diagnostics.Tools": "4.0.1",
        "System.Dynamic.Runtime": "4.0.11",
        "System.IO.FileSystem": "4.0.1",
        "System.Linq": "4.1.0",
        "System.Net.NameResolution": "4.0.0",
        "System.Net.Requests": "4.0.11",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Runtime.Serialization.Xml": "4.1.1",
        "System.Threading.Tasks": "4.0.11",
        "System.Threading.Tasks.Parallel": "4.0.1",
        "System.Threading.Thread": "4.0.0",
        "System.Threading.ThreadPool": "4.0.10",
        "System.Xml.XmlDocument": "4.0.1",
        "System.Xml.XmlSerializer": "4.0.11",
        "System.Data.Common": "4.1.0"
      }
    }
  }
}